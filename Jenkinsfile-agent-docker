pipeline {
    agent any

    stages {
    stage('Build Java') {
    agent {
        docker { image 'maven:3.9-eclipse-temurin-21' }
            }
            steps {
        sh 'mvn clean package'

                // Cette commande "sort" le fichier du conteneur avant qu'il ne soit dÃ©truit
                archiveArtifacts artifacts: 'target/*.jar', fingerprint: true
            }
        }
    }
}
