pipeline {
    agent any
    tools {
        // Install the Maven  version configured as "Maven3.9.12" and add it to the path.
        maven "Maven3.9.12"
        jdk 'Corretto-17'
    }
    stages {
        stage('Build') {
            steps {
                // jmeter
                sh "jmeter -Jjmeter.save.saveservice.output_format=xml -Jjmeter.save.saveservice.response_data.on_error=true -n -t petclinic_test_plan.jmx -l testresult.jlt"
            }

        }
    }
}
