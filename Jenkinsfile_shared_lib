@Library('ma-lib') _

pipeline {
    agent any
    tools {
        // Install the Maven version configured as "Maven3.9.12" and add it to the path.
        maven "Maven3.9.12"
        jdk 'Corretto-17'
    }
    stages {
        stage('Build') {
            steps {
                //git 'https://github.com/crunchy-devops/spring-framework-petclinic.git'
                // Utilisation de notre fonction personnalis√©e
                buildMaven('clean package')
            }
        }
    }
    post {
        success {
            notifySlack('SUCCESS')
        }
        failure {
            notifySlack('FAILURE')
        }
    }
}
